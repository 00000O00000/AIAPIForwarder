        "name": "openai-main",
        "endpoint": "https://api.openai.com/v1",
        "api_key": "replace-with-openai-api-key",
        "model": "gpt-4o",
        "format": "openai",
        "priority": 1,
        "weight": 10,
        "rate_limit": {
          "requests_per_period": 1000,
          "tokens_per_period": 2000000,
          "period_cron": "0 0 * * *"
        },
        "retry": 2,
        "timeout": 60,
        "stream_support": true,
        "non_stream_support": true,
        "enabled": true,
        "custom_headers": {
          "X-Project": "gateway-demo"
        },
        "max_context_length": 128000
      },
      {
        "name": "openai-responses-backup",
        "endpoint": "https://api.openai.com/v1",
        "api_key": "replace-with-openai-api-key",
        "model": "gpt-4o",
        "format": "openai-response",
        "priority": 2,
        "weight": 8,
        "rate_limit": {
          "requests_per_period": 500,
          "tokens_per_period": 1000000,
          "period_cron": "0 0 * * *"
